/**app.wxss**/

page {
  background: #f2f2f2;
  font-size: 24rpx;
}

.app {
  width: 100%;
  height: auto;
  overflow: hidden;
}

.slide-image {
  width: 100%;
  height: 360rpx;
}

.more, .nomore {
  height: 60rpx;
  /* text-align: center; */
  line-height: 60rpx;
  font-size: 26rpx;
  background: #fff;
  width: 100%;
  display: flex;
  justify-content: center;
  /* margin: 0 auto; */
  border-top: 1rpx solid #f2f2f2;
}

.nomore {
  color: #ccc;
}

.jszc {
  height: 80rpx;
  width: 100%;
  text-align: center;
  line-height: 80rpx;
  overflow: hidden;
  color: #ccc;
}

.kongbai {
  width: 100%;
  height: 120rpx;
}

.footer {
  height: 100rpx;
  width: 100%;
  position: fixed;
  bottom: 0;
  background: #fff;
  border-top: 1rpx solid #e2e2e2;
}

.footer .ft_item {
  width: 25%;
  height: 100%;
  text-align: center;
  float: left;
  color: #666;
}

.footer .ft_item .image {
  height: 45rpx;
  width: 45rpx;
  margin: 0 auto;
  display: block;
  margin-top: 10rpx;
  font-size: 45rpx;
}

.footer .ft_item image {
  height: 45rpx;
  width: 45rpx;
  margin: 0 auto;
  display: block;
  margin-top: 10rpx;
}

.footer  .ft_item text {
  text-align: center;
  font-size: 22rpx;
  display: block;
  margin: 0 auto;
  margin-top: 10rpx;
}

.bdbt{
  border-bottom: 1rpx solid #e2e2e2;
}

.tblt {
  border-top: 1rpx #bdbdbd solid;
}

.tblb {
  border-bottom: 1rpx #bdbdbd solid;
}

.tbll {
  border-left: 1rpx #bdbdbd solid;
}

.tblr {
  border-right: 1rpx #bdbdbd solid;
}

.kefutel {
  position: fixed;
  z-index: 100;
  right: 10rpx;
  bottom: 200rpx;
  background: rgba(0, 0, 0, 0.6);
  color: #fff;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50rpx;
  text-align: center;
}

.kefutel text {
  font-size: 50rpx;
  line-height: 80rpx;
}

@font-face {
	font-family: 'iconfont';
	src: url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
		url('iconfont.ttf') format('truetype');
	font-weight: normal;
	font-style: normal;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-shouye:before { content: "\e632"; }

.icon-bianji:before { content: "\e605"; }

.icon-icon-yxj-empty-my:before { content: "\e651"; }

.icon-kefu:before { content: "\e622"; }

.icon-baomu:before { content: "\e600"; }

.icon-dingdan:before { content: "\e820"; }

/** 小程序去除滚动条 **/

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.table_form{
  height: auto;
  overflow: hidden;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
  background: #fff;
  margin-top: 20rpx;
}

.table_form .input{
  height: 80rpx;
  line-height: 80rpx;
  font-size: 26rpx;
}

.table_form .input text{
  float: left;
  width: 20%;
  margin-left: 2%;
}
.table_form .input input{
  float: left;
  width: 70%;
  margin-left: 5rpx;
  height: 80rpx;font-size: 24rpx;
}
.table_form .input radio-group{
  float: left;margin-left: 5rpx;
  font-size: 24rpx; color: #666;
}
.table_form .input radio{
  transform:scale(0.8);
}

.table_form .photolist{
  height: auto;
  overflow: hidden;
  width: 96%;
  margin-left: 2%;
}
.table_form .photolist .title{
  width: 20%;
  float: left;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 26rpx;
}
.table_form .photolist .action{
  width: 70%;
  float: left;margin-left: 10rpx;
  height: 100rpx;
}
.table_form .photolist .action image{
    height: 100rpx;
    width: 100rpx;
}
.table_form .photolist .photos{
  float: left;
  margin-top: 20rpx;
  width:100%;
  height: auto;
  overflow: hidden;
}


.table_form .photolist .photos .item {
  float: left;
  width: 150rpx;
  height: 150rpx;
  margin-left: 20rpx;
  margin-top: 20rpx;
}

.table_form .photolist image{
  float: left;
  width: 120rpx;
  height: 120rpx;
  margin-right: 10rpx;
}
.table_form .picker{
  height: 80rpx;
  line-height: 80rpx;
  font-size: 26rpx;
}
.table_form .map{
  height: 80rpx;
  line-height: 80rpx;
  overflow: hidden;
}
.table_form .map .lt{
  float: left;
  width: 20%;
  margin-left: 2%;
  font-size: 26rpx;

}
.table_form .map .ct{
  width: 60%;
  height: 80rpx;
  overflow: hidden;
  float: left;
  margin-left: 5rpx;
}
.table_form .map image{
  width: 40rpx;
  height: 40rpx;
  float: right;margin-right: 10rpx;
  margin-top: 20rpx;
}

.table_form .picker .lt{
  float: left;
  width: 20%;
  margin-left: 2%;
}
.table_form .picker .ct{
  float: left;
  width: 60%;
  margin-left: 5rpx;
  font-size: 24rpx;
  color: #666;
  text-align: right;
}

.table_form .picker .rt{
  float: right;
  margin-right: 10rpx; height: 40rpx;
  width: 40rpx;
  margin-top: 20rpx;
}


.table_form .textarea{
  height: 200rpx;
  width: 96%;
  margin-left: 2%;
  font-size: 26rpx;
}

.table_form .textarea text{
  line-height: 200rpx;
  width: 20%;
  float: left;
}


.table_form .textarea textarea{
  width: 70%;
  margin-left: 5rpx;
  height: 150rpx;
  margin-top: 25rpx;
  float: left;
  border: 1rpx dotted   #f2f2f2;
  font-size: 24rpx;
  padding: 10rpx;
}

.table_form .logo{
  height: 100rpx;
  font-size: 26rpx;
}

.table_form .logo .lt{
  float: left;width: 20%;
  margin-left: 2%;
  height: 100rpx;
  line-height: 100rpx;
}

.table_form .logo image{
  float: left;
  margin-left: 5rpx;
  margin-right: 25rpx;
  height: 100rpx;
  width: 100rpx;
}